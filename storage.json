{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "VM": {
            "type": "array",
            "defaultValue": ["JSON-VM2","JSON-VML"],
            "metadata": {
                "description": "description"
            }
        }
    },
    "variables": {
        "vmArray":{
            "copy":{
                "name": "loop",
                "count": "[length(parameters('VM'))]",
                "input":{
                    "id": "[concat(parameters('VM'),copyIndex())]"
                }
            }
        }
    },
    "resources": [],
    "outputs": {
        "message": {
            "type": "array",
            "value": "[variables('vmArray')]"
        }
    }
}